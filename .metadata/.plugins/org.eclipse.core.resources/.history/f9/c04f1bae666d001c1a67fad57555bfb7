package aula07a;
public class Luta extends Lutador{

	public Luta(String no, String naci, int idade, float alt, float pe, int vit, int der, int emp) {
		super(no, naci, idade, alt, pe, vit, der, emp);
	}

	private String desafiado;
	private String desafiante;
	private int raunds;
	private String aprovada;
	
	public void marcarLuta(dd = lutador) {
		desafiado = dd;
		
	}
	
	public void lutar() {
		
	}

	public String getDesafiado() {
		return desafiado;
	}

	public void setDesafiado(String desafiado) {
		this.desafiado = desafiado;
	}

	public String getDesafiante() {
		return desafiante;
	}

	public void setDesafiante(String desafiante) {
		this.desafiante = desafiante;
	}

	public int getRaunds() {
		return raunds;
	}

	public void setRaunds(int raunds) {
		this.raunds = raunds;
	}

	public String getAprovada() {
		return aprovada;
	}

	public void setAprovada(String aprovada) {
		this.aprovada = aprovada;
	}
	
}
