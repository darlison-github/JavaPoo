package aula07a;
public class Luta{

	private Lutador desafiado;
	private Lutador desafiante;
	private int raunds;
	private boolean aprovada;
	
	public void marcarLuta() {
		
	}
	
	public void lutar() {
		
	}

	public String getDesafiado() {
		return desafiado;
	}

	public void setDesafiado(String desafiado) {
		this.desafiado = desafiado;
	}

	public String getDesafiante() {
		return desafiante;
	}

	public void setDesafiante(String desafiante) {
		this.desafiante = desafiante;
	}

	public int getRaunds() {
		return raunds;
	}

	public void setRaunds(int raunds) {
		this.raunds = raunds;
	}

	public String getAprovada() {
		return aprovada;
	}

	public void setAprovada(String aprovada) {
		this.aprovada = aprovada;
	}
	
}
